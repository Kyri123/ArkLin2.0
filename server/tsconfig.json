{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDirs": [
      "./src",
      "../src"
    ],
    "types": [
      "node"
    ],
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "../buildserver/",
    "strict": true,
    "moduleResolution": "Node",
    "importHelpers": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "noImplicitAny": false,
    "isolatedModules": false,
    "lib": [
      "ESNext"
    ],
    "typeRoots": [
      "../node_modules/@types",
      "./src/Types",
      "./src/Types/global.d.ts",
      "../src/Types",
      "../src/Shared/Type",
      "../src/Shared/Api"
    ],
    "sourceMap": true,
    "baseUrl": "../"
  },
  "files": [
    "./src/Types/global.d.ts"
  ],
  "include": [
    "./src/**/*.ts",
    "../src/Shared/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}